    "custom_multiple_media_eVmAmW": {
      "type": "custom-multiple-media",
      "blocks": {
        "media_ENgCaG": {
          "type": "media",
          "settings": {
            "width": "",
            "img": "shopify://shop_images/Mask_group_9_934bde02-8666-4bf4-b937-078f790fa5d6.png",
