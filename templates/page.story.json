    "custom_multiple_media_NiLLD4": {
      "type": "custom-multiple-media",
      "blocks": {
        "media_p4rdE8": {
          "type": "media",
          "settings": {
            "width": "",
            "img": "shopify://shop_images/Mask_group_3_253c12c9-4cbe-43b0-83dd-868244a8d67b.png",
