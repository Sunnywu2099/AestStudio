    "custom_multiple_media_xH8GJY": {
      "type": "custom-multiple-media",
      "blocks": {
        "media_MjAA9N": {
          "type": "media",
          "settings": {
            "width": "",
            "img": "shopify://shop_images/Mask_group_2_7d39d54e-5667-4cc2-bb61-e0396fcff115.png",
            "use_video": false,
