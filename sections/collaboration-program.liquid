<style>
  #shopify-section-{{section.id}} {
    padding: {{section.settings.pt}}px 0 {{section.settings.pb}}px;
  }
  #shopify-section-{{section.id}} .section-title{
    color: #25282A;
    font-family: Courier;
    font-size: 22px;
    font-weight: 700;
    line-height: 30.8px; /* 140% */
    letter-spacing: 1.1px;
    text-align: center;
    margin-bottom: 60px;
  }
  @media screen and (max-width: 999px) {
    #shopify-section-{{section.id}} {
      padding: {{section.settings.pt_m}}px 0 {{section.settings.pb_m}}px;
    }
    #shopify-section-{{section.id}} .section-title{
      font-size: 18px;
      line-height: 22px;
      letter-spacing: 1.1px;
      margin-bottom: 30px;
    }
  }
  @media screen and (max-width: 699px) {

  }
</style>

<div class="container container--xl">
  {% if section.settings.page_desc != blank %}
    <h2 class="section-title">{{ section.settings.page_desc }}</h2>
  {% endif %}
  <div class="media-wrapper">


  </div>
</div>

<script>
  document.addEventListener('DOMContentLoaded', function () {
    
  });
  
</script>

{% schema %}
  {
    "name": "Collaboration Program",
    "class": "collaboration-program",
    "tag": "section",
    "disabled_on": {
      "groups": ["footer", "header"]
    },
    "settings": [
      {
        "type": "text",
        "id": "page_desc",
        "label": "Page description"
      },
      {
        "type": "range",
        "id": "pt",
        "min": 0,
        "max": 160,
        "step": 2,
        "label": "Padding top",
        "default": 120,
        "unit": "px"
      },
      {
        "type": "range",
        "id": "pb",
        "min": 0,
        "max": 160,
        "step": 2,
        "label": "Padding bottom",
        "default": 120,
        "unit": "px"
      },
      {
        "type": "range",
        "id": "pt_m",
        "min": 0,
        "max": 100,
        "step": 1,
        "label": "Padding top Mobile",
        "default": 60,
        "unit": "px"
      },
      {
        "type": "range",
        "id": "pb_m",
        "min": 0,
        "max": 100,
        "step": 1,
        "label": "Padding bottom Mobile",
        "default": 60,
        "unit": "px"
      }
    ],
    "blocks": [
      {
        "type": "item",
        "name": "Item",
        "settings": [
          {
            "type": "text",
            "id": "title",
            "label": "Title",
            "default": "Product Introduction"
          },
          {
            "type": "image_picker",
            "id": "img",
            "label": "Image PC"
          },
          {
            "type": "image_picker",
            "id": "img_m",
            "label": "Image Mobile"
          },
          {
            "type": "url",
            "id": "url",
            "label": "Url"
          }
        ]
      }
    ],
    "presets": [
      {
        "name": "Collaboration Program"
      }
    ]
  }
{% endschema %}